          ]
        },
        {
          "match": "requests.local-application.*",
          "metric": "${0}",
          "labels": []
        },
        {
          "match": "worker.*.*",
          "metric": "${0}_${1}",
          "labels": []
        }
      ]
    },
    "nb-threads": 16,
    "allowed-worker-delay-ms": 23000,
    "allowed-workers-in-delay": 1
