    },
    "file": {
      "cdrFieldsPatterns": {
        "default": [
          "MSISDN",
          "ServiceSessionId",
          "MsgType",
          "MsgSessionId",
          "MsgId",
          "MsgDetails",
          "Servicedetails",
          "LogLineTime",
          "callAnswered",
          "callEnded",
          "callDuration"
        ]
      },
      "cdrFileOptions": {
        "default": {
          "path": "/appl/aep/var/cdr/poc/",
          "maxAgeInMinutes": 840,
          "useFooter": true,
          "estimatedCdrLineLength": 20,
          "maxNbOfLines": 10000,
          "closeSuffix": ".done",
          "useJsonFormat": false,
          "footer": [
            "T",
            "GENDATE",
            "GENTIME",
            "current:MSISDN"
          ],
          "header": [
            "H1",
            "GENDATE",
            "GENTIME",
            "1",
            "\n",
            "H2",
            "MSISDN",
            "ServiceSessionId",
            "MsgType",
            "MsgSessionId",
            "MsgId",
            "MsgDetails",
            "Servicedetails",
            "LogLineTime",
            "callAnswered",
            "callEnded",
            "callDuration"
          ],
          "prefix": "cdr",
          "useHeader": true,
          "maxSizeInBytes": 1024000,
          "openSuffix": ".open",
          "separatorEscapeSequenceInFieldContent": "â€™",
          "useLinesCount": false,
          "fieldSeparator": ","
        }
      }
    },
    "stats": {
      "jmx-enabled": "true",
      "prometheus-enabled": "true",
      "prometheus-http-port": 50711,
      "prometheus-export-hotspot-metrics": "true",
